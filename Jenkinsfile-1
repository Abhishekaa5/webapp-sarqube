pipeline {
  agent any
  
  stages {
    stage('build && SonarQube analysis') {
            steps {
                withSonarQubeEnv('sonarqube7.1') {
                    // Optionally use a Maven environment you've configured already
                    withMaven(maven:'Apache Maven 3.6.3') {
                        sh 'mvn clean package sonarqube:sonarqube'
                    }
                }
            }
        }
  }
}
